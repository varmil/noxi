{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": [
      "jest",
      "@testing-library/jest-dom"
    ],
    "paths": {
      "@/*": [
        "./@/*"
      ],
      "apis/*": [
        "./apis/*"
      ],
      "app/*": [
        "./app/*"
      ],
      "config/*": [
        "./config/*"
      ],
      "features/*": [
        "./features/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "hooks/*": [
        "./hooks/*"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "types/*": [
        "./types/*"
      ],
      "utils/*": [
        "./utils/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
