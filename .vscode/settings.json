{
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.fixAll.eslint"
  ],
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },

  "typescript.preferences.importModuleSpecifier": "non-relative",
  "eslint.workingDirectories": ["./web", "./backend"],
  "eslint.useFlatConfig": true,

  "files.associations": {
    "*.css": "tailwindcss"
  },
  "exportall.config.folderListener": [
    "/backend/closed-api-server/src/domain/youtube",
    "/backend/closed-api-server/src/domain/youtube/channel",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api",
    "/backend/closed-api-server/src/domain/youtube/video",
    "/backend/closed-api-server/src/domain/youtube/channel/branding-settings",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/search-videos",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/search-channels",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/channels",
    "/backend/closed-api-server/src/domain/youtube/playlist",
    "/backend/closed-api-server/src/domain/youtube/chart/channel",
    "/backend/closed-api-server/src/domain/youtube/chart",
    "/backend/closed-api-server/src/domain/country",
    "/backend/closed-api-server/src/domain/time-period",
    "/backend/closed-api-server/src/domain/youtube/country"
  ],
  "exportall.config.relExclusion": [
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/search-videos/VideoTranslator.ts",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/channels/ChannelTranslator.ts",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/playlist-items/PlaylistItemSchema.ts",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/search-videos/SearchVideosAPISchema.ts",
    "/backend/closed-api-server/src/infrastructure/service/youtube-data-api/channels/ChannelAPISchema.ts"
  ]
}
