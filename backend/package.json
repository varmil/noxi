{
  "name": "backend",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate --schema prisma/schema",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check-duplicate-imports": "node scripts/check-duplicate-imports.js",
    "dev": "nest start --watch",
    "build": "nest build --webpack",
    "prod": "node dist/apps/closed-api-server/main",
    "prisma:dev": "prisma migrate dev --schema prisma/schema",
    "prisma:deploy": "prisma migrate deploy --schema prisma/schema",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:coverage": "vitest run --coverage",
    "app": "zx ./scripts/standalone-apps/factory.mjs"
  },
  "dependencies": {
    "@googleapis/youtube": "^30.1.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@prisma/client": "^7.1.0",
    "@ubie/nslog": "^1.2.1",
    "axios": "^1.10.0",
    "bignumber.js": "^9.3.0",
    "cache-manager": "^7.2.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.0",
    "dayjs": "^1.11.13",
    "fast-xml-parser": "^5.3.3",
    "firebase-admin": "^13.6.0",
    "keyv": "^5.5.5",
    "passport-jwt": "^4.0.1",
    "prisma-json-types-generator": "^3.6.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "stripe": "^19.2.0",
    "twitter-api-v2": "^1.23.2",
    "xss": "^1.0.15",
    "zod": "^4.1.12",
    "zx": "^8.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.5",
    "@types/compression": "^1.8.1",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.3",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-prettier": "^5.5.1",
    "fast-check": "^4.4.0",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.16"
  }
}
