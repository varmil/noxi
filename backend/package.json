{
  "name": "backend",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate --schema prisma/schema",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check-duplicate-imports": "node scripts/check-duplicate-imports.js",
    "dev": "nest start --watch",
    "build": "nest build --webpack",
    "prod": "node dist/apps/closed-api-server/main",
    "prisma:dev": "prisma migrate dev --schema prisma/schema",
    "prisma:deploy": "prisma migrate deploy --schema prisma/schema",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "test:coverage": "jest --coverage",
    "app": "zx ./scripts/standalone-apps/factory.mjs"
  },
  "dependencies": {
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^3.0.0",
    "@googleapis/youtube": "^30.2.0",
    "@keyv/redis": "^5.1.5",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation-express": "^0.57.1",
    "@opentelemetry/instrumentation-http": "^0.209.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.55.0",
    "@opentelemetry/resources": "^2.3.0",
    "@opentelemetry/sdk-node": "^0.209.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@ubie/nslog": "^1.2.1",
    "axios": "^1.13.2",
    "bignumber.js": "^9.3.0",
    "cache-manager": "^7.2.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.0",
    "dayjs": "^1.11.13",
    "fast-xml-parser": "^5.3.3",
    "firebase-admin": "^13.6.0",
    "passport-jwt": "^4.0.1",
    "prisma-json-types-generator": "^4.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "stripe": "^20.1.2",
    "twitter-api-v2": "^1.28.0",
    "xss": "^1.0.15",
    "zod": "^4.3.5",
    "zx": "^8.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/compression": "^1.8.1",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-prettier": "^5.5.1",
    "fast-check": "^4.5.3",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
